uired \r\n          :disabled=\"isLoading\"\r\n          @input=\"clearError\"\r\n          placeholder=\"请输入用户名\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"email\">邮箱:</label>\r\n        <input \r\n          type=\"email\" \r\n          id=\"email\" \r\n          v-model.trim=\"email\" \r\n          required \r\n          :disabled=\"isLoading\"\r\n          @input=\"clearError\"\r\n          placeholder=\"请输入邮箱\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"password\">密码:</label>\r\n        <div class=\"password-input\">\r\n          <input \r\n            :type=\"showPassword ? 'text' : 'password'\" \r\n            id=\"password\" \r\n            v-model=\"password\" \r\n            required \r\n            :disabled=\"isLoading\"\r\n            @input=\"clearError\"\r\n            placeholder=\"请输入密码\"\r\n          />\r\n          <i \r\n            class=\"fas\" \r\n            :class=\"showPassword ? 'fa-eye-slash' : 'fa-eye'\"\r\n            @click=\"togglePassword\"\r\n          ></i>\r\n        </div>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"confirmPassword\">确认密码:</label>\r\n        <div class=\"password-input\">\r\n          <input \r\n            :type=\"showConfirmPassword ? 'text' : 'password'\" \r\n            id=\"confirmPassword\" \r\n            v-model=\"confirmPassword\" \r\n            required \r\n            :disabled=\"isLoading\"\r\n            @input=\"clearError\"\r\n            placeholder=\"请再次输入密码\"\r\n          />\r\n          <i \r\n            class=\"fas\" \r\n            :class=\"showConfirmPassword ? 'fa-eye-slash' : 'fa-eye'\"\r\n            @click=\"toggleConfirmPassword\"\r\n          ></i>\r\n        </div>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"captcha\">验证码:</label>\r\n        <div class=\"captcha-group\">\r\n          <div class=\"captcha-input\">\r\n            <input \r\n              type=\"text\" \r\n              id=\"captcha\" \r\n              v-model=\"userInputCaptcha\" \r\n              required \r\n              :disabled=\"isLoading\"\r\n              @input=\"clearError\"\r\n              maxlength=\"4\"\r\n              placeholder=\"请输入验证码\"\r\n            />\r\n          </div>\r\n          <div class=\"captcha-code\" @click=\"refreshCaptcha\" title=\"点击刷新验证码\">\r\n            {{ displayCaptcha }}\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <button type=\"submit\" :disabled=\"isLoading || !isFormValid\">\r\n        <span v-if=\"isLoading\">\r\n          <i class=\"fas fa-spinner fa-spin\"></i> 注册中...\r\n        </span>\r\n        <span v-else>注册</span>\r\n      </button>\r\n      <div v-if=\"errorMessage\" class=\"error-container\">\r\n        <i class=\"fas fa-exclamation-circle\"></i>\r\n        {{ errorMessage }}\r\n      </div>\r\n      <div class=\"login-link\">\r\n        已有账号? <router-link to=\"/login\">点击登录</router-link>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { API_URLS, axiosConfig } from '../config/api';\r\n\r\nconst api = axios.create(axiosConfig);\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      username: '',\r\n      email: '',\r\n      password: '',\r\n      confirmPassword: '',\r\n      userInputCaptcha: '',\r\n      displayCaptcha: '',\r\n      correctCaptcha: '',\r\n      errorMessage: '',\r\n      isLoading: false,\r\n      showPassword: false,\r\n      showConfirmPassword: false\r\n    };\r\n  },\r\n  computed: {\r\n    isFormValid() {\r\n      return this.username.trim() && \r\n             this.email.trim() && \r\n             this.password && \r\n             this.password === this.confirmPassword &&\r\n             this.userInputCaptcha.length === 4;\r\n    }\r\n  },\r\n  created() {\r\n    // 初始化生成随机验证码\r\n    this.refreshCaptcha();\r\n  },\r\n  methods: {\r\n    clearError() {\r\n      this.errorMessage = '';\r\n    },\r\n    togglePassword() {\r\n      this.showPassword = !this.showPassword;\r\n    },\r\n    toggleConfirmPassword() {\r\n      this.showConfirmPassword = !this.showConfirmPassword;\r\n    },\r\n    // 生成4位随机数字验证码\r\n    refreshCaptcha() {\r\n      // 生成四位随机数字 (1000-9999)\r\n      const randomNum = Math.floor(1000 + Math.random() * 9000);\r\n      // 转为字符串\r\n      const captchaText = randomNum.toString();\r\n      \r\n      // 设置正确的验证码值和显示值\r\n      this.correctCaptcha = captchaText;\r\n      this.displayCaptcha = captchaText;\r\n      \r\n      // 清空用户输入\r\n      this.userInputCaptcha = '';\r\n      \r\n      // 输出到控制台用于调试\r\n      console.log(`新验证码已生成: ${this.correctCaptcha}`);\r\n    },\r\n    async handleSubmit() {\r\n      if (!this.isFormValid) {\r\n        if (this.password !== this.confirmPassword) {\r\n          this.errorMessage = '两次输入的密码不一致';\r\n          return;\r\n        }\r\n        return;\r\n      }\r\n      \r\n      // 检查验证码\r\n      if (this.userInputCaptcha !== this.correctCaptcha) {\r\n        console.log('验证码不匹配:', {\r\n          用户输入: this.userInputCaptcha,\r\n          正确值: this.correctCaptcha\r\n        });\r\n        this.errorMessage = '验证码错误，请重新输入';\r\n        this.refreshCaptcha();\r\n        return;\r\n      }\r\n      \r\n      console.log('验证码匹配成功，提交注册请求');\r\n      \r\n      this.isLoading = true;\r\n      this.errorMessage = '';\r\n      \r\n      try {\r\n        const registerData = {\r\n          username: this.username.trim(),\r\n          email: this.email.trim(),\r\n          password: this.password,\r\n          captcha: this.userInputCaptcha\r\n        };\r\n        \r\n        console.log('正在发送注册请求:', {\r\n          url: `${axiosConfig.baseURL}${API_URLS.register}`,\r\n          username: registerData.username,\r\n          email: registerData.email\r\n        });\r\n        \r\n        const response = await api.post(API_URLS.register, registerData);\r\n        \r\n        console.log('服务器响应:', response.data);\r\n        \r\n        if (response.data && response.data.message === '注册成功') {\r\n          // 注册成功，跳转到登录页\r\n          this.$router.push('/login?registered=true');\r\n          return;\r\n        }\r\n        \r\n        // 如果没有成功消息但服务器返回了消息\r\n        if (response.data && response.data.message) {\r\n          this.errorMessage = response.data.message;\r\n          this.refreshCaptcha();\r\n          return;\r\n        }\r\n      } catch (error) {\r\n        console.error('注册过程中发生错误:', error);\r\n        \r\n        // 处理API错误响应\r\n        if (error.response) {\r\n          const status = error.response.status;\r\n          const errorData = error.response.data;\r\n          \r\n          switch (status) {\r\n            case 400:\r\n              this.errorMessage = errorData.message || '注册信息有误，请检查输入';\r\n              break;\r\n            case 409:\r\n              this.errorMessage = '用户名已存在，请选择其他用户名';\r\n              break;\r\n            case 500:\r\n              this.errorMessage = '服务器错误，请稍后重试';\r\n              break;\r\n            default:\r\n              this.errorMessage = errorData?.message || '注册失败，请稍后重试';\r\n          }\r\n          this.refreshCaptcha();\r\n        } else {\r\n          this.errorMessage = '无法连接到服务器，请检查网络连接';\r\n          this.refreshCaptcha();\r\n        }\r\n      } finally {\r\n        this.isLoading = false;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.register-form {\r\n  background-color: white;\r\n  padding: 2rem;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n  width: 100%;\r\n  max-width: 400px;\r\n}\r\n\r\nh2 {\r\n  text-align: center;\r\n  margin-bottom: 1.5rem;\r\n  color: #333;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 1rem;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 0.5rem;\r\n  font-weight: 500;\r\n  color: #555;\r\n}\r\n\r\ninput {\r\n  width: 100%;\r\n  padding: 0.75rem;\r\n  border: 1px solid #ddd;\r\