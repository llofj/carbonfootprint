          icon: 'water.png',\n          unit: '立方米',\n          factor: 0.9\n        }, {\n          name: '电',\n          icon: 'electricity.png',\n          unit: '立方米',\n          factor: 0.7\n        }, {\n          name: '天然气',\n          icon: 'natural_gas.png',\n          unit: '立方米',\n          factor: 2.1\n        }],\n        '行': [{\n          name: '轮船',\n          icon: 'ship.png',\n          unit: '公里',\n          factor: 0.03\n        }, {\n          name: '公交车',\n          icon: 'bus.png',\n          unit: '公里',\n          factor: 0.1\n        }, {\n          name: '私家电车',\n          icon: 'electric-car.png',\n          unit: '公里',\n          factor: 0.05\n        }, {\n          name: '私家油车',\n          icon: 'gas-car.png',\n          unit: '公里',\n          factor: 0.25\n        }, {\n          name: '火车',\n          icon: 'train.png',\n          unit: '公里',\n          factor: 0.06\n        }, {\n          name: '飞机',\n          icon: 'plane.png',\n          unit: '公里',\n          factor: 0.25\n        }],\n        '用': [{\n          name: '塑料袋',\n          icon: 'plastic.png',\n          unit: '个',\n          factor: 0.02\n        }, {\n          name: '纸巾',\n          icon: 'tissue.png',\n          unit: '包(10张)',\n          factor: 0.8\n        }, {\n          name: '电子设备',\n          icon: 'device.png',\n          unit: '小时',\n          factor: 1.2\n        }]\n      },\n      calculationItems: [],\n      totalEmission: 0,\n      offsetTrees: 0.3\n    };\n  },\n  created: function created() {\n    // 检查用户是否登录\n    var token = localStorage.getItem('token');\n    if (token) {\n      this.isLoggedIn = true;\n      this.username = localStorage.getItem('username') || '用户';\n      this.loadUserCarbonReduction();\n    }\n  },\n  methods: {\n    setActiveCategory: function setActiveCategory(category) {\n      this.activeCategory = category;\n    },\n    // 加载用户减碳总量\n    loadUserCarbonReduction: function loadUserCarbonReduction() {\n      var _this = this;\n      return _asyncToGenerator(/*#__PURE__*/_regeneratorRuntime().mark(function _callee() {\n        var token, response, data, achievementResponse, _data;\n        return _regeneratorRuntime().wrap(function _callee$(_context) {\n          while (1) switch (_context.prev = _context.next) {\n            case 0:\n              _context.prev = 0;\n              token = localStorage.getItem('token');\n              if (token) {\n                _context.next = 4;\n                break;\n              }\n              return _context.abrupt(\"return\");\n            case 4:\n              _context.next = 6;\n              return fetch('http://localhost:5000/api/leaderboard/user', {\n                headers: {\n                  'Authorization': \"Bearer \".concat(token)\n                }\n              });\n            case 6:\n              response = _context.sent;\n              if (!response.ok) {\n                _context.next = 14;\n                break;\n              }\n              _context.next = 10;\n              return response.json();\n            case 10:\n              data = _context.sent;\n              if (!(data && data.carbon_reduction)) {\n                _context.next = 14;\n                break;\n              }\n              _this.userCarbonReduction = parseFloat(data.carbon_reduction) || 0;\n              return _context.abrupt(\"return\");\n            case 14:\n              _context.next = 16;\n              return fetch('http://localhost:5000/api/achievement/carbon-reduction', {\n                headers: {\n                  'Authorization': \"Bearer \".concat(token)\n                }\n              });\n            case 16:\n              achievementResponse = _context.sent;\n              if (!achievementResponse.ok) {\n                _context.next = 22;\n                break;\n              }\n              _context.next = 20;\n              return achievementResponse.json();\n            case 20:\n              _data = _cont