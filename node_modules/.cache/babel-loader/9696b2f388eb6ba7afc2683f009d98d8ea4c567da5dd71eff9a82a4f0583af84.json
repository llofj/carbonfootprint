Captcha));\n    },\n    // 刷新验证码\n    refreshCaptcha: function refreshCaptcha() {\n      this.generateCaptcha();\n      this.captcha = ''; // 清空用户输入的验证码\n    },\n    handleSubmit: function handleSubmit() {\n      var _this = this;\n      return _asyncToGenerator(/*#__PURE__*/_regeneratorRuntime().mark(function _callee() {\n        var loginData, response, redirect, status, errorData, _redirect;\n        return _regeneratorRuntime().wrap(function _callee$(_context) {\n          while (1) switch (_context.prev = _context.next) {\n            case 0:\n              if (_this.isFormValid) {\n                _context.next = 2;\n                break;\n              }\n              return _context.abrupt(\"return\");\n            case 2:\n              // 调试日志\n              console.log('验证码比对:', {\n                输入: _this.captcha,\n                输入类型: _typeof(_this.captcha),\n                显示: _this.displayCaptcha,\n                显示类型: _typeof(_this.displayCaptcha),\n                比对结果: String(_this.captcha) === String(_this.displayCaptcha)\n              });\n\n              // 验证验证码 - 使用String转换确保两边都是字符串类型进行比较\n              if (!(String(_this.captcha) !== String(_this.displayCaptcha))) {\n                _context.next = 7;\n                break;\n              }\n              _this.errorMessage = '验证码错误，请重新输入';\n              _this.refreshCaptcha(); // 出错时刷新验证码\n              return _context.abrupt(\"return\");\n            case 7:\n              _this.isLoading = true;\n              _this.errorMessage = '';\n              _context.prev = 9;\n              loginData = {\n                username: _this.username.trim(),\n                password: _this.password.trim(),\n                captcha: _this.captcha.trim()\n              };\n              console.log('正在发送登录请求:', {\n                url: \"\".concat(axiosConfig.baseURL).concat(API_URLS.login),\n                username: loginData.username\n              });\n              _context.prev = 12;\n              _context.next = 15;\n              return api.post(API_URLS.login, loginData);\n            case 15:\n              response = _context.sent;\n              console.log('服务器响应:', response.data);\n              if (!(response.data && response.data.token)) {\n                _context.next = 24;\n                break;\n              }\n              // 登录成功\n              localStorage.setItem('token', response.data.token);\n              localStorage.setItem('username', _this.username.trim());\n              redirect = _this.$route.query.redirect || '/';\n              _context.next = 23;\n              return _this.$router.push(redirect);\n            case 23:\n              return _context.abrupt(\"return\");\n            case 24:\n              if (!(response.data && response.data.message)) {\n                _context.next = 28;\n                break;\n              }\n              _this.errorMessage = response.data.message;\n              _this.refreshCaptcha(); // 出错时刷新验证码\n              return _context.abrupt(\"return\");\n            case 28:\n              _context.next = 49;\n              break;\n            case 30:\n              _context.prev = 30;\n              _context.t0 = _context[\"catch\"](12);\n              console.error('API调用错误:', _context.t0);\n\n              // 处理API错误响应\n              if (!_context.t0.response) {\n                _context.next = 48;\n                break;\n              }\n              status = _context.t0.response.status;\n              errorData = _context.t0.response.data;\n              _context.t1 = status;\n              _context.next = _context.t1 === 401 ? 39 : _context.t1 === 403 ? 41 : _context.t1 === 500 ? 43 : 45;\n              break;\n            case 39:\n              _this.errorMessage = '账号或密码错误';\n              return _context.abrupt(\"break\", 46);\n            case 41:\n              _this.errorMessage = '验证码错误';\n              return _context.abrupt(\"break\", 46);\n            case 43:\n