     _this4.lastAction = response.data;\n              _context4.next = 19;\n              break;\n            case 15:\n              _context4.prev = 15;\n              _context4.t0 = _context4[\"catch\"](0);\n              console.error('Error training pet:', _context4.t0);\n              alert('训练失败，请重试');\n            case 19:\n            case \"end\":\n              return _context4.stop();\n          }\n        }, _callee4, null, [[0, 15]]);\n      }))();\n    },\n    levelUp: function levelUp() {\n      var _this5 = this;\n      return _asyncToGenerator(/*#__PURE__*/_regeneratorRuntime().mark(function _callee5() {\n        var token, response;\n        return _regeneratorRuntime().wrap(function _callee5$(_context5) {\n          while (1) switch (_context5.prev = _context5.next) {\n            case 0:\n              _context5.prev = 0;\n              if (!(_this5.pet.coins < _this5.levelUpCost || _this5.pet.health === '营养不良')) {\n                _context5.next = 4;\n                break;\n              }\n              alert('金币不足或健康状态不佳，需要100金币');\n              return _context5.abrupt(\"return\");\n            case 4:\n              token = localStorage.getItem('token');\n              _context5.next = 7;\n              return axios.post('http://localhost:5000/api/pet/level-up', {}, {\n                headers: {\n                  Authorization: \"Bearer \".concat(token)\n                }\n              });\n            case 7:\n              response = _context5.sent;\n              _this5.pet = response.data.pet;\n              _this5.experienceToNextLevel = response.data.experienceToNextLevel;\n\n              // 触发升级动画\n              _this5.playAnimation('grow');\n              alert(response.data.message);\n              _this5.lastAction = response.data;\n              _context5.next = 19;\n              break;\n            case 15:\n              _context5.prev = 15;\n              _context5.t0 = _context5[\"catch\"](0);\n              console.error('Error leveling up pet:', _context5.t0);\n              alert('操作失败，请重试');\n            case 19:\n            case \"end\":\n              return _context5.stop();\n          }\n        }, _callee5, null, [[0, 15]]);\n      }))();\n    },\n    playAnimation: function playAnimation(animationType) {\n      var _this6 = this;\n      // 清除之前的动画\n      if (this.animationTimeout) {\n        clearTimeout(this.animationTimeout);\n      }\n\n      // 设置新动画\n      this.animationClass = animationType;\n\n      // 设置定时器移除动画类\n      this.animationTimeout = setTimeout(function () {\n        _this6.animationClass = '';\n      }, 1500);\n    }\n  }\n};","map":{"version":3,"names":["axios","data","pet","name","level","health","coins","experience","steps","simulatedSteps","carbonReduction","coinsEarned","animationClass","animationTimeout","experienceToNextLevel","feedingCost","trainingCost","levelUpCost","lastAction","computed","nextLevelExperience","experiencePercentage","created","_this","_asyncToGenerator","_regeneratorRuntime","mark","_callee","token","response","wrap","_callee$","_context","prev","next","localStorage","getItem","$router","push","abrupt","get","headers","Authorization","concat","sent","t0","console","error","stop","methods","getHealthStatusClass","submitSteps","_this2","_callee2","_callee2$","_context2","alert","post","stepsProcessed","playAnimation","toFixed","feedPet","_this3","_callee3","_callee3$","_context3","message","trainPet","_this4","_callee4","_callee4$","_context4","levelUp","_this5","_callee5","_callee5$","_context5","animationType","_this6","clearTimeout","setTimeout"],"sources":["C:\\Users\\ASUS\\Desktop\\carbonfootprint\\src\\components\\VirtualPet.vue"],"sourcesContent":["<template>\r\n  <div class=\"virtual-pet-page\">\r\n    <h2>我的虚拟宠物</h2>\r\n    \r\n    <div class=\"pet-container\">\r\n      <div class=\"pet-info-card\">\r\n        <div class=\"pet-name\">{{ pet.name }}</div>\r\n        <div class=\"pet-stats\">\r\n          <div class=\"stat\">\r\n            <i class=\"fa fa-star\"></i>\r\n            <span>等级: {{ pet.level }}</span>\r\n          </div>\r\n          <div class=\"stat\">\r\n            <i class=\"fa fa-heart\"></i>\r\n            <span>健康状态: \r\n              <span :class=\"['health-status', getHealthStatusClass(pet.health)]\">\r\n                {{ pet.health }}\r\n              </span>\r\n            </span>\r\n          </div>\r\n          <div class=\"stat\">\r\n            <i class=\"fa fa-coins\"></i>\r\n            <span>金币: {{ pet.coins }}</span>\r\n          </div>\r\n          <div class=\"stat\">\r\n            <i class=\"fa fa-chart-line\"></i>\r\n            <span>经验值: {{ pet.experience }} / {{ experienceToNextLevel }}</span>\r\n          </div>\r\n        </div>\r\n        <div class=\"experience-bar\">\r\n          <div class=\"experience-progress\" :style=\"{ width: experiencePercentage + '%' }\"></div>\r\n          <span class=\"experience-text\">{{ pet.experience }} / {{ nextLevelExperience }}</span>\r\n        </div>\r\n      </div>\r\n      \r\n      <div class=\"pet-animation-container\">\r\n        <div class=\"pet-animation\" :class=\"animationClass\">\r\n          <img src=\"@/assets/nailong.png\" alt=\"奶龙\" class=\"pet-image\">\r\n        </div>\r\n      </div>\r\n      \r\n      <div class=\"pet-actions\">\r\n        <button @click=\"feedPet\" :disabled=\"pet.coins < feedingCost\" class=\"action-btn feed-btn\">\r\n          <i class=\"fa fa-utensils\"></i> 喂食 ({{ feedingCost }}金币)\r\n        </button>\r\n        <button @click=\"trainPet\" :disabled=\"pet.coins < trainingCost || pet.health === '营养不良'\" class=\"action-btn train-btn\">\r\n          <i class=\"fa fa-dumbbell\"></i> 训练 ({{ trainingCost }}金币)\r\n        </button>\r\n        <button @click=\"levelUp\" :disabled=\"pet.coins < levelUpCost || pet.health === '营养不良'\" class=\"action-btn level-up-btn\">\r\n          <i class=\"fa fa-arrow-up\"></i> 升级 ({{ levelUpCost }}金币)\r\n        </button>\r\n      </div>\r\n    </div>\r\n    \r\n    <div v-if=\"pet.health === '营养不良'\" class=\"warning-message\">\r\n      <p>⚠️ 警告：宠物处于营养不良状态！</p>\r\n      <p>请先喂食提升健康状态，才能进行训练和升级。</p>\r\n    </div>\r\n    \r\n    <div class=\"steps-container\">\r\n      <h3>微信步数</h3>\r\n      <p class=\"steps-info\">今日步数: {{ steps }} 步</p>\r\n      <p class=\"carbon-info\" v-if=\"carbonReduction\">减少碳排放: {{ carbonReduction.toFixed(2) }} kg CO2</p>\r\n      <p class=\"coins-info\" v-if=\"coinsEarned\">获得金币: {{ coinsEarned }}</p>\r\n      \r\n      <div class=\"steps-input\">\r\n        <input type=\"number\" v-model=\"simulatedSteps\" placeholder=\"输入步数\" min=\"1000\" max=\"30000\" />\r\n        <button @click=\"submitSteps\" class=\"submit-steps-btn\">\r\n          <i class=\"fa fa-walking\"></i> 提交步数\r\n        </button>\r\n      </div>\r\n      \r\n      <div class=\"steps-note\">\r\n        <p>注: 在实际应用中，步数将从微信API获取</p>\r\n      </div>\r\n    </div>\r\n    \r\n    <div v-if=\"lastAction\" class=\"action-result\">\r\n      <p>{{ lastAction.message }}</p>\r\n      <p v-if=\"lastAction.carbonReduction\">\r\n        减少碳排放: {{ lastAction.carbonReduction }}kg CO2\r\n      </p>\r\n      <p v-if=\"lastAction.coinsEarned\">\r\n        获得金币: {{ lastAction.coinsEarned }}\r\n      </p>\r\n      <p v-if=\"lastAction.experienceGained\">\r\n        获得经验值: {{ lastAction.experienceGained }}\r\n      </p>\r\n    </div>\r\n    \r\n    <div class=\"info-section\">\r\n      <h3>步行减排说明</h3>\r\n      <ul>\r\n        <li>每走1000步可减少约0.1kg碳排放</li>\r\n        <li>每减少1kg碳排放可获得10个金币</li>\r\n        <li>金币可用于喂食、训练和获得经验值</li>\r\n        <li>喂食可提升宠物健康状态</li>\r\n        <li>花费二十金币训练宠物，根据健康状态获得不同经验值：</li>\r\n        <li class=\"training-info\">\r\n          <div class=\"health-status\">\r\n            <strong>健康状态：</strong>\r\n            <ul>\r\n              <li>健康：20%概率获得1-10经验值，40%概率获得11-30经验值，40%概率获得31-40经验值</li>\r\n              <li>良好：40%概率获得1-10经验值，40%概率获得11-30经验值，20%概率获得31-40经验值</li>\r\n              <li>较差：60%概率获得1-10经验值，20%概率获得11-30经验值，20%概率获得31-40经验值</li>\r\n            </ul>\r\n          </div>\r\n        </li>\r\n        <li>每100经验值可提升1级</li>\r\n        <li>宠物等级越高，减排效果越好</li>\r\n      </ul>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      pet: {\r\n        name: '奶龙',\r\n        level: 1,\r\n        health: '健康',\r\n        coins: 0,\r\n        experience: 0\r\n      },\r\n      steps: 0,\r\n      simulatedSteps: 8000,\r\n      carbonReduction: 0,\r\n      coinsEarned: 0,\r\n      animationClass: '',\r\n      animationTimeout: null,\r\n      experienceToNextLevel: 100,\r\n      feedingCost: 10,\r\n      trainingCost: 20,\r\n      levelUpCost: 100,\r\n      lastAction: null\r\n    };\r\n  },\r\n  computed: {\r\n    nextLevelExperience() {\r\n      return this.pet.level * 100;\r\n    },\r\n    experiencePercentage() {\r\n      return (this.pet.experience / this.nextLevelExperience) * 100;\r\n    }\r\n  },\r\n  async created() {\r\n    try {\r\n      const token = localStorage.getItem('token');\r\n      if (!token) {\r\n        this.$router.push('/');\r\n        return;\r\n      }\r\n      \r\n      // 获取宠物信息\r\n      const response = await axios.get('http://localhost:5000/api/pet', {\r\n        headers: { Authorization: `Bearer ${token}` }\r\n      });\r\n      \r\n      this.pet = response.data;\r\n      this.experienceToNextLevel = response.data.experienceToNextLevel;\r\n    } catch (error) {\r\n      console.error('Error fetching pet data:', error);\r\n    }\r\n  },\r\n  methods: {\r\n    getHealthStatusClass(health) {\r\n      return {\r\n        'health-healthy': health === '健康',\r\n        'health-good': health === '良好',\r\n        'health-poor': health === '较差',\r\n        'health-malnourished': health === '营养不良'\r\n      }\r\n    },\r\n    async submitSteps() {\r\n      try {\r\n        if (this.simulatedSteps < 1000) {\r\n          alert('步数太少，请至少输入1000步');\r\n          return;\r\n        }\r\n        \r\n        const token = localStorage.getItem('token');\r\n        const response = await axios.post('http://localhost:5000/api/pet/steps', {\r\n          steps: this.simulatedSteps\r\n        }, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n        \r\n        this.pet = response.data.pet;\r\n        this.steps = response.data.stepsProcessed;\r\n        this.carbonReduction = response.data.carbonReduction;\r\n        this.coinsEarned = response.data.coinsEarned;\r\n        \r\n        // 触发获得金币的动画\r\n        this.playAnimation('bounce');\r\n        \r\n        // 显示成功消息\r\n        alert(`步数提交成功！减少了${response.data.carbonReduction.toFixed(2)}kg碳排放，获得了${response.data.coinsEarned}金币！`);\r\n        \r\n        this.lastAction = response.data;\r\n      } catch (error) {\r\n        console.error('Error submitting steps:', error);\r\n        alert('提交步数失败，请重试');\r\n      }\r\n    },\r\n    async feedPet() {\r\n      try {\r\n        if (this.pet.coins < this.feedingCost) {\r\n          alert('金币不足，无法喂食');\r\n          return;\r\n        }\r\n        \r\n        const token = localStorage.getItem('token');\r\n        const response = await axios.post('http://localhost:5000/api/pet/feed', {}, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n        \r\n        this.pet = response.data.pet;\r\n        \r\n        // 触发喂食动画\r\n        this.playAnimation('wiggle');\r\n        \r\n        alert(response.data.message);\r\n        \r\n        this.lastAction = response.data;\r\n      } catch (error) {\r\n        console.error('Error feeding pet:', error);\r\n        alert('喂食失败，请重试');\r\n      }\r\n    },\r\n    async trainPet() {\r\n      try {\r\n        if (this.pet.coins < this.trainingCost || this.pet.health === '营养不良') {\r\n          alert('金币不足或健康状态不佳，无法训练');\r\n          return;\r\n        }\r\n        \r\n        const token = localStorage.getItem('token');\r\n        const response = await axios.post('http://localhost:5000/api/pet/train', {}, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n        \r\n        this.pet = response.data.pet;\r\n        this.experienceToNextLevel = response.data.experienceToNextLevel;\r\n        \r\n        // 触发训练动画\r\n        this.playAnimation('shake');\r\n        \r\n        alert(response.data.message);\r\n        \r\n        this.lastAction = response.data;\r\n      } catch (error) {\r\n        console.error('Error training pet:', error);\r\n        alert('训练失败，请重试');\r\n      }\r\n    },\r\n    async levelUp() {\r\n      try {\r\n        if (this.pet.coins < this.levelUpCost || this.pet.health === '营养不良') {\r\n          alert('金币不足或健康状态不佳，需要100金币');\r\n          return;\r\n        }\r\n        \r\n        const token = localStorage.getItem('token');\r\n        const response = await axios.post('http://localhost:5000/api/pet/level-up', {}, {\r\n          headers: { Authorization: `Bearer ${token}` }\r\n        });\r\n        \r\n        this.pet = response.data.pet;\r\n        this.experienceToNextLevel = response.data.experienceToNextLevel;\r\n        \r\n        // 触发升级动画\r\n        this.playAnimation('grow');\r\n        \r\n        alert(response.data.message);\r\n        \r\n        this.lastAction = response.data;\r\n      } catch (error) {\r\n        console.error('Error leveling up pet:', error);\r\n        alert('操作失败，请重试');\r\n      }\r\n    },\r\n    playAnimation(animationType) {\r\n      // 清除之前的动画\r\n      if (this.animationTimeout) {\r\n        clearTimeout(this.animationTimeout);\r\n      }\r\n      \r\n      // 设置新动画\r\n      this.animationClass = animationType;\r\n      \r\n      // 设置定时器移除动画类\r\n      this.animationTimeout = setTimeout(() => {\r\n        this.animationClass = '';\r\n      }, 1500);\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.virtual-pet-page {\r\n  max-width: 800px;\r\n  margin: 0 auto;\r\n  padding: 20px;\r\n  font-