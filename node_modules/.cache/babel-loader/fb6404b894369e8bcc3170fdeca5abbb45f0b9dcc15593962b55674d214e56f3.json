n      console.log('刷新验证码 (模拟)');\n    },\n    // 为每个数字生成随机样式，增加视觉干扰\n    getRandomStyle: function getRandomStyle() {\n      var rotation = Math.floor(Math.random() * 10) - 5;\n      var fontSize = 14 + Math.floor(Math.random() * 4);\n      var letterSpacing = Math.floor(Math.random() * 3) - 1;\n      return {\n        transform: \"rotate(\".concat(rotation, \"deg)\"),\n        fontSize: \"\".concat(fontSize, \"px\"),\n        letterSpacing: \"\".concat(letterSpacing, \"px\"),\n        display: 'inline-block',\n        fontWeight: 'bold',\n        margin: '0 2px'\n      };\n    },\n    handleSubmit: function handleSubmit() {\n      var _this = this;\n      return _asyncToGenerator(/*#__PURE__*/_regeneratorRuntime().mark(function _callee() {\n        var registerData, response, status, errorData;\n        return _regeneratorRuntime().wrap(function _callee$(_context) {\n          while (1) switch (_context.prev = _context.next) {\n            case 0:\n              if (_this.isFormValid) {\n                _context.next = 5;\n                break;\n              }\n              if (!(_this.password !== _this.confirmPassword)) {\n                _context.next = 4;\n                break;\n              }\n              _this.errorMessage = '两次输入的密码不一致';\n              return _context.abrupt(\"return\");\n            case 4:\n              return _context.abrupt(\"return\");\n            case 5:\n              if (!(_this.captchaInput !== FIXED_CAPTCHA)) {\n                _context.next = 10;\n                break;\n              }\n              console.log(\"\\u9A8C\\u8BC1\\u7801\\u6BD4\\u5BF9: \\u7528\\u6237\\u8F93\\u5165=\\\"\".concat(_this.captchaInput, \"\\\"\\uFF0C\\u6B63\\u786E\\u503C=\\\"\").concat(FIXED_CAPTCHA, \"\\\"\\uFF0C\\u4E0D\\u5339\\u914D\"));\n              _this.errorMessage = '验证码错误，请重新输入';\n              _this.captchaInput = '';\n              return _context.abrupt(\"return\");\n            case 10:\n              console.log(\"验证码校验通过，准备提交注册请求\");\n              _this.isLoading = true;\n              _this.errorMessage = '';\n              _context.prev = 13;\n              registerData = {\n                username: _this.username.trim(),\n                email: _this.email.trim(),\n                password: _this.password,\n                captcha: _this.captchaInput\n              };\n              console.log('正在发送注册请求:', {\n                url: \"\".concat(axiosConfig.baseURL).concat(API_URLS.register),\n                username: registerData.username,\n                email: registerData.email\n              });\n              _context.next = 18;\n              return api.post(API_URLS.register, registerData);\n            case 18:\n              response = _context.sent;\n              console.log('服务器响应:', response.data);\n              if (!(response.data && response.data.message === '注册成功')) {\n                _context.next = 23;\n                break;\n              }\n              // 注册成功，跳转到登录页\n              _this.$router.push('/login?registered=true');\n              return _context.abrupt(\"return\");\n            case 23:\n              if (!(response.data && response.data.message)) {\n                _context.next = 27;\n                break;\n              }\n              _this.errorMessage = response.data.message;\n              _this.simulateRefreshCaptcha();\n              return _context.abrupt(\"return\");\n            case 27:\n              _context.next = 50;\n              break;\n            case 29:\n              _context.prev = 29;\n              _context.t0 = _context[\"catch\"](13);\n              console.error('注册过程中发生错误:', _context.t0);\n\n              // 处理API错误响应\n              if (!_context.t0.response) {\n                _context.next = 48;\n                break;\n              }\n              status = _context.t0.response.status;\n              errorData = _context.t0.response.data;\n              _context.t1 = status;\n              _context.next = _context.t1 === 400 ? 38 : _context.t1 === 409 ? 40 : _context.t1 === 500 ? 42 : 44;\n              break;\n            case 38:\n              _this.errorMessage = errorData.message || '注册信息有误，请检查输入';\n              return _context.abrupt(\"break\", 45);\n            case 40:\n              _this.errorMessage = '用户名已存在，请选择其他用户名';\n              return _context.abrupt(\"break\", 45);\n            case 42:\n              _this.errorMessage = '服务器错误，请稍后重试';\n              return _context.abrupt(\"break\", 45);\n            case 44:\n              _this.errorMessage = (errorData === null || errorData === void 0 ? void 0 : errorData.message) || '注册失败，请稍后重试';\n            case 45:\n              _this.simulateRefreshCaptcha();\n              _context.next = 50;\n              break;\n            case 48:\n              _this.errorMessage = '无法连接到服务器，请检查网络连接';\n              _this.simulateRefreshCaptcha();\n            case 50:\n              _context.prev = 50;\n              _this.isLoading = false;\n              return _context.finish(50);\n            case 53:\n            case \"end\":\n              return _context.stop();\n          }\n        }, _callee, null, [[13, 29, 50, 53]]);\n      }))();\n    }\n  }\n};","map":{"version":3,"names":["axios","API_URLS","axiosConfig","FIXED_CAPTCHA","api","create","data","username","email","password","confirmPassword","captchaInput","captchaDisplay","errorMessage","isLoading","showPassword","showConfirmPassword","refreshCount","computed","isFormValid","trim","length","methods","clearError","togglePassword","toggleConfirmPassword","simulateRefreshCaptcha","captchaElement","document","querySelector","classList","add","setTimeout","remove","console","log","getRandomStyle","rotation","Math","floor","random","fontSize","letterSpacing","transform","concat","display","fontWeight","margin","handleSubmit","_this","_asyncToGenerator","_regeneratorRuntime","mark","_callee","registerData","response","status","errorData","wrap","_callee$","_context","prev","next","abrupt","captcha","url","baseURL","register","post","sent","message","$router","push","t0","error","t1","finish","stop"],"sources":["D:\\EcoPaw\\carbonfootprint\\src\\components\\RegisterForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"register-form\">\r\n    <h2>注册新用户</h2>\r\n    <form @submit.prevent=\"handleSubmit\">\r\n      <div class=\"form-group\">\r\n        <label for=\"username\">用户名:</label>\r\n        <input \r\n          type=\"text\" \r\n          id=\"username\" \r\n          v-model.trim=\"username\" \r\n          required \r\n          :disabled=\"isLoading\"\r\n          @input=\"clearError\"\r\n          placeholder=\"请输入用户名\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"email\">邮箱:</label>\r\n        <input \r\n          type=\"email\" \r\n          id=\"email\" \r\n          v-model.trim=\"email\" \r\n          required \r\n          :disabled=\"isLoading\"\r\n          @input=\"clearError\"\r\n          placeholder=\"请输入邮箱\"\r\n        />\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"password\">密码:</label>\r\n        <div class=\"password-input\">\r\n          <input \r\n            :type=\"showPassword ? 'text' : 'password'\" \r\n            id=\"password\" \r\n            v-model=\"password\" \r\n            required \r\n            :disabled=\"isLoading\"\r\n            @input=\"clearError\"\r\n            placeholder=\"请输入密码\"\r\n          />\r\n          <i \r\n            class=\"fas\" \r\n            :class=\"showPassword ? 'fa-eye-slash' : 'fa-eye'\"\r\n            @click=\"togglePassword\"\r\n          ></i>\r\n        </div>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"confirmPassword\">确认密码:</label>\r\n        <div class=\"password-input\">\r\n          <input \r\n            :type=\"showConfirmPassword ? 'text' : 'password'\" \r\n            id=\"confirmPassword\" \r\n            v-model=\"confirmPassword\" \r\n            required \r\n            :disabled=\"isLoading\"\r\n            @input=\"clearError\"\r\n            placeholder=\"请再次输入密码\"\r\n          />\r\n          <i \r\n            class=\"fas\" \r\n            :class=\"showConfirmPassword ? 'fa-eye-slash' : 'fa-eye'\"\r\n            @click=\"toggleConfirmPassword\"\r\n          ></i>\r\n        </div>\r\n      </div>\r\n      <div class=\"form-group\">\r\n        <label for=\"captcha\">验证码:</label>\r\n        <div class=\"captcha-group\">\r\n          <div class=\"captcha-input\">\r\n            <input \r\n              type=\"text\" \r\n              id=\"captcha\" \r\n              v-model=\"captchaInput\" \r\n              required \r\n              :disabled=\"isLoading\"\r\n              @input=\"clearError\"\r\n              maxlength=\"4\"\r\n              pattern=\"[0-9]{4}\"\r\n              placeholder=\"请输入验证码\"\r\n            />\r\n          </div>\r\n          <div \r\n            class=\"captcha-code\" \r\n            @click=\"simulateRefreshCaptcha\" \r\n            title=\"当前验证码为0000\"\r\n          >\r\n            <span\r\n              v-for=\"(digit, index) in captchaDisplay\"\r\n              :key=\"index\"\r\n              :style=\"getRandomStyle()\"\r\n            >{{ digit }}</span>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      <button type=\"submit\" :disabled=\"isLoading || !isFormValid\">\r\n        <span v-if=\"isLoading\">\r\n          <i class=\"fas fa-spinner fa-spin\"></i> 注册中...\r\n        </span>\r\n        <span v-else>注册</span>\r\n      </button>\r\n      <div v-if=\"errorMessage\" class=\"error-container\">\r\n        <i class=\"fas fa-exclamation-circle\"></i>\r\n        {{ errorMessage }}\r\n      </div>\r\n      <div class=\"login-link\">\r\n        已有账号? <router-link to=\"/login\">点击登录</router-link>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios';\r\nimport { API_URLS, axiosConfig } from '../config/api';\r\n\r\n// 固定验证码常量 - 与后端匹配\r\nconst FIXED_CAPTCHA = \"0000\";\r\n\r\nconst api = axios.create(axiosConfig);\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      username: '',\r\n      email: '',\r\n      password: '',\r\n      confirmPassword: '',\r\n      captchaInput: '',\r\n      captchaDisplay: FIXED_CAPTCHA,\r\n      errorMessage: '',\r\n      isLoading: false,\r\n      showPassword: false,\r\n      showConfirmPassword: false,\r\n      refreshCount: 0\r\n    };\r\n  },\r\n  computed: {\r\n    isFormValid() {\r\n      return this.username.trim() && \r\n             this.email.trim() && \r\n             this.password && \r\n             this.password === this.confirmPassword &&\r\n             this.captchaInput && \r\n             this.captchaInput.length === 4;\r\n    }\r\n  },\r\n  methods: {\r\n    clearError() {\r\n      this.errorMessage = '';\r\n    },\r\n    togglePassword() {\r\n      this.showPassword = !this.showPassword;\r\n    },\r\n    toggleConfirmPassword() {\r\n      this.showConfirmPassword = !this.showConfirmPassword;\r\n    },\r\n    // 模拟刷新验证码效果，但实际值仍为0000\r\n    simulateRefreshCaptcha() {\r\n      this.refreshCount++;\r\n      this.captchaInput = '';\r\n      \r\n      // 添加刷新动画\r\n      const captchaElement = document.querySelector('.captcha-code');\r\n      if (captchaElement) {\r\n        captchaElement.classList.add('refreshing');\r\n        setTimeout(() => {\r\n          captchaElement.classList.remove('refreshing');\r\n        }, 500);\r\n      }\r\n      \r\n      console.log('刷新验证码 (模拟)');\r\n    },\r\n    // 为每个数字生成随机样式，增加视觉干扰\r\n    getRandomStyle() {\r\n      const rotation = Math.floor(Math.random() * 10) - 5;\r\n      const fontSize = 14 + Math.floor(Math.random() * 4);\r\n      const letterSpacing = Math.floor(Math.random() * 3) - 1;\r\n      \r\n      return {\r\n        transform: `rotate(${rotation}deg)`,\r\n        fontSize: `${fontSize}px`,\r\n        letterSpacing: `${letterSpacing}px`,\r\n        display: 'inline-block',\r\n        fontWeight: 'bold',\r\n        margin: '0 2px'\r\n      };\r\n    },\r\n    async handleSubmit() {\r\n      if (!this.isFormValid) {\r\n        if (this.password !== this.confirmPassword) {\r\n          this.errorMessage = '两次输入的密码不一致';\r\n          return;\r\n        }\r\n        return;\r\n      }\r\n      \r\n      // 检查验证码 - 固定值\"0000\"\r\n      if (this.captchaInput !== FIXED_CAPTCHA) {\r\n        console.log(`验证码比对: 用户输入=\"${this.captchaInput}\"，正确值=\"${FIXED_CAPTCHA}\"，不匹配`);\r\n        this.errorMessage = '验证码错误，请重新输入';\r\n        this.captchaInput = '';\r\n        return;\r\n      }\r\n      \r\n      console.log(\"验证码校验通过，准备提交注册请求\");\r\n      \r\n      this.isLoading = true;\r\n      this.errorMessage = '';\r\n      \r\n      try {\r\n        const registerData = {\r\n          username: this.username.trim(),\r\n          email: this.email.trim(),\r\n          password: this.password,\r\n          captcha: this.captchaInput\r\n        };\r\n        \r\n        console.log('正在发送注册请求:', {\r\n          url: `${axiosConfig.baseURL}${API_URLS.register}`,\r\n          username: registerData.username,\r\n          email: registerData.email\r\n        });\r\n        \r\n        const response = await api.post(API_URLS.register, registerData);\r\n        \r\n        console.log('服务器响应:', response.data);\r\n        \r\n        if (response.data && response.data.message === '注册成功') {\r\n          // 注册成功，跳转到登录页\r\n          this.$router.push('/login?registered=true');\r\n          return;\r\n        }\r\n        \r\n        // 如果没有成功消息但服务器返回了消息\r\n        if (response.data && response.data.message) {\r\n          this.errorMessage = response.data.message;\r\n          this.simulateRefreshCaptcha();\r\n          return;\r\n        }\r\n      } catch (error) {\r\n        console.error('注册过程中发生错误:', error);\r\n        \r\n        // 处理API错误响应\r\n        if (error.response) {\r\n          const status = error.response.status;\r\n          const errorData = error.response.data;\r\n          \r\n          switch (status) {\r\n            case 400:\r\n              this.errorMessage = errorData.message || '注册信息有误，请检查输入';\r\n              break;\r\n            case 409:\r\n              this.errorMessage = '用户名已存在，请选择其他用户名';\r\n              break;\r\n            case 500:\r\n              this.errorMessage = '服务器错误，请稍后重试';\r\n              break;\r\n            default:\r\n              this.errorMessage = errorData?.message || '注册失败，请稍后重试';\r\n          }\r\n          this.simulateRefreshCaptcha();\r\n        } else {\r\n          this.errorMessage = '无法连接到服务器，请检查网络连接';\r\n          this.simulateRefreshCaptcha();\r\n        }\r\n      } finally {\r\n        this.isLoading = false;\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.register-form {\r\n  background-color: white;\r\n  padding: 2rem;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 6px rgba(0, 0, 0, 0.1);\r\n  width: 100%;\r\n  max-width: 400px;\r\n}\r\n\r\nh2 {\r\n  text-align: center;\r\n  margin-bottom: 1.5rem;\r\n  color: #333;\r\n}\r\n\r\n.form-group {\r\n  margin-bottom: 1rem;\r\n}\r\n\r\nlabel {\r\n  display: block;\r\n  margin-bottom: 0.5rem;\r\n  font-weight: 500;\r\n  color: #555;\r\n}\r\n\r\ninput {\r\n  width: 100%;\r\n  padding: 0.75rem;\r\n  border: 1px solid #ddd;\r\n  border-radius: 4px;\r\n  font-size: 1rem;\r\n  transition: border-color 0.3s;\r\n}\r\n\r\ninput:focus {\r\n  border-color: #4CAF50;\r\n  outline: none;\r\n}\r\n\r\n.password-input {\r\n  position: relative;\r\n}\r\n\r\n.password-input i {\r\n  position: absolute;\r\n  right: 10px;\r\n  top: 50%;\r\n  transform: translateY(-50%);\r\n  cursor: pointer;\r\n  color: #777;\r\n}\r\n\r\n.captcha-group {\r\n  display: flex;\r\n  gap: 10px;\r\n}\r\n\r\n.captcha-input {\r\n  flex: 1;\r\n}\r\n\r\n.captcha-code {\r\n  background-color: #f0f0f0;\r\n  padding: 0.75rem;\r\n  border-radius: 4px;\r\n  font-family: monospace;\r\n  font-weight: bold;\r\