置显示值\n      this.captchaDisplay = this.captchaValue.toString();\n      // 重置用户输入\n      this.captchaInput = null;\n      console.log('生成新验证码:', {\n        验证码数值: this.captchaValue,\n        验证码类型: _typeof(this.captchaValue),\n        显示值: this.captchaDisplay\n      });\n    },\n    // 提交登录\n    handleSubmit: function handleSubmit() {\n      var _this = this;\n      return _asyncToGenerator(/*#__PURE__*/_regeneratorRuntime().mark(function _callee() {\n        var loginData, response, redirect, status, errorData, _redirect;\n        return _regeneratorRuntime().wrap(function _callee$(_context) {\n          while (1) switch (_context.prev = _context.next) {\n            case 0:\n              if (_this.isFormValid) {\n                _context.next = 2;\n                break;\n              }\n              return _context.abrupt(\"return\");\n            case 2:\n              // 验证码比对 (直接比较数字)\n              console.log('验证码比对:', {\n                用户输入: _this.captchaInput,\n                用户输入类型: _typeof(_this.captchaInput),\n                系统验证码: _this.captchaValue,\n                系统验证码类型: _typeof(_this.captchaValue),\n                比对结果: _this.captchaInput === _this.captchaValue\n              });\n              if (!(_this.captchaInput !== _this.captchaValue)) {\n                _context.next = 7;\n                break;\n              }\n              _this.errorMessage = '验证码错误，请重新输入';\n              _this.generateNewCaptcha();\n              return _context.abrupt(\"return\");\n            case 7:\n              _this.isLoading = true;\n              _this.errorMessage = '';\n              _context.prev = 9;\n              loginData = {\n                username: _this.username.trim(),\n                password: _this.password,\n                captcha: _this.captchaInput.toString() // 转为字符串发送\n              };\n              console.log('正在发送登录请求:', {\n                url: \"\".concat(axiosConfig.baseURL).concat(API_URLS.login),\n                usernam